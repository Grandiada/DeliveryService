{
  "compilerOptions": {
    "target": "es6",
    "plugins": [{
      "name": "typescript-plugin-css-modules"
    }],
    "lib": [
      "es5",
      "es2015",
      "es2016",
      "dom"
    ],
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "strictFunctionTypes": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "module": "esnext",
    "skipLibCheck": true,
  },
  "exclude": [
    "node_modules"
  ],
}